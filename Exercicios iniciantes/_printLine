import java.util.Scanner;
import java.util.Locale;

public class _printLine {
    public static void main(String[] args) {
        /* Fazer um programa para ler o código de uma peça 1, o número de peças 1, o valor unitário de cada peça 1, o
      código de uma peça 2, o número de peças 2 e o valor unitário de cada peça 2. Calcule e mostre o valor a ser pago. */
       Locale.setDefault(Locale.US);

      Scanner sc = new Scanner();
     
      int codP1,codP2,numP1,nump2;
      double valorP1, valorP2, valorTot;

      System.out.print("Dados do primeiro produdo:");
      codP1 = sc.nextInt();
      numP1 = sc.nextInt();
      valorP1 = nextDouble();
      codP2 = sc.nextInt();
      numP2 = sc.nextInt();
      valorP2 = nextDouble();

      valorTot = ((numP1*valorP1)+(numP2*valorP2));

      System.out.println("Dados do primeiro produdo:");
      System.out.printl(codP1);
      System.out.printl(numP1);
      System.out.println(valorP1);
      System.out.println("Dados do segundo produdo:");
      System.out.println(codP2);
      System.out.println(numP2);
      System.out.println(valorP2);

      System.out.printf("VALOR A PAGAR: R$ %.2f", valorTot );


        sc.close();
    }
  }